  "scripts": {
    "info:ionic": "ionic info",
    "start": "ng serve -o --port=1418 --host 0.0.0.0 --poll 50",
    "start:lab": "ionic serve --lab -o --host 0.0.0.0 --lab-host 0.0.0.0",
